<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/schedule"
    app:startDestination="@id/specializationFragment">
    <fragment
        android:id="@+id/specializationFragment"
        android:name="com.example.gym.fragment.SpecializationFragment"
        android:label="Специальности"
        tools:layout="@layout/fragment_specialization" >
        <action
            android:id="@+id/action_specializationFragment_to_calendarFragment"
            app:destination="@id/calendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.example.gym.fragment.CalendarFragment"
        android:label="Запись"
        tools:layout="@layout/fragment_calendar" />
</navigation>